				{ "name": "voice", "desc": "Voice object from <a href=\"./#mik_play_sound\">play_sound</a>" },
				{ "name": "value", "desc": "The volume/gain value (0.0 - 1.0)", "default": "nil" }
			]
		},
		{
			"ns": "mik",
			"func": "master_volume",
			"desc": "Gets/Sets the master volume/gain. If no <u>value</u> argument is passed, it will act as a getter.",
			"args": [
				{ "name": "value", "desc": "The volume/gain value (0.0 - 1.0)", "default": "nil" }
			]
		},
		{
			"ns": "mik",
			"func": "pitch",
			"desc": "Gets/Sets the voice's pitch. If no <u>value</u> argument is passed, it will act as a getter.",
			"args": [
				{ "name": "voice", "desc": "Voice object from <a href=\"./#mik_play_sound\">play_sound</a>" },
				{ "name": "value", "desc": "The pitch value (0.1 - 10.0)", "default": "nil" }
			]
		},
		{
			"ns": "mik",
			"func": "pan",
			"desc": "Gets/Sets the voice's pan. If no <u>value</u> argument is passed, it will act as a getter.",
			"args": [
				{ "name": "voice", "desc": "Voice object from <a href=\"./#mik_play_sound\">play_sound</a>" },
				{ "name": "value", "desc": "The pan value (-1.0 - 1.0)", "default": "nil" }
			]
		},

		{
			"ns": "math",
			"func": "lerp",
			"desc": "Linear interpolation.",
			"args": [
				{ "name": "a", "desc": "From" },
				{ "name": "b", "desc": "To" },
				{ "name": "t", "desc": "Factor" }
			]
		},
		{
			"ns": "math",
			"func": "clamp",
			"desc": "Value clamping.",
			"args": [
				{ "name": "v", "desc": "Initial value" },
				{ "name": "min", "desc": "Minimum" },
				{ "name": "max", "desc": "Maximum" }
			]
		},

		{
			"title": "Buttons",
			"desc": "Button constant strings.",
			"list": [
				{ "left": "Left button. Mapped to <b>Left Arrow Key</b> and <b>S Key</b>." },
				{ "right": "Right button. Mapped to <b>Right Arrow Key</b> and <b>F Key</b>." },
				{ "up": "Up button. Mapped to <b>Up Arrow Key</b> and <b>E Key</b>." },
				{ "down": "Down button. Mapped to <b>Down Arrow Key</b> and <b>D Key</b>." },
				{ "a": "A button. Mapped to <b>Z Key</b> and <b>K Key</b>." },
